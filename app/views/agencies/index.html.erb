<div class="flex-container ">
  <section class="cards agency-display">
  <h1 class="results">Search Results:</h1>
  <h2 ><%= @label %></h2>
  <%= image_tag("Map.PNG", :width => "70%", :height => "auto", :display => "block") %>
    <% @agencies.each do |agency| %>
      <article class="card">
        <div class="card-content">
          <h2><%= link_to agency.name, agency_path(agency) %> - Rating:  <%= agency.rating %></h2>
        </div>
      </article>
    <% end %>
  </section>
</div>

