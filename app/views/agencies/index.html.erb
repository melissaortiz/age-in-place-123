<div class="centered results">
  <h1>Search Results:</h1>
  <h2><%= @label %></h2>
  <section class="cards">
  <%= image_tag("Map.PNG", :width => "100%", :height => "auto", :display => "block") %>
    <% @agencies.each do |agency| %>
      <article class="card">
        <div class="card-content">
          <h2><%= link_to agency.name, agency_path(agency) %> - Rating:  <%= agency.rating %></h2>
        </div>
      </article>
    <% end %>
  </section>
</div>

